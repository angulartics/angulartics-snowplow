    "snowplow",
    "analytics",
    "plugin",
    "angular",
    "angulartics"
  ],
  "author": "Tim Elfelt <timelf123@gmail.com> (https://github.com/timelf123)",
  "contributors": [
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
